<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shravan.alphab3t">

    <application
        android:allowBackup="true"
        android:label="Alphabet"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.shravan.alphab3t.difficultyActivity" />
        <activity android:name="com.shravan.alphab3t.highScoresActivity" />
        <activity android:name="com.shravan.alphab3t.themesActivity" />
        <activity android:name="com.shravan.alphab3t.tutorialActivity" />
        <activity android:name="com.shravan.alphab3t.endActivity" />
        <activity android:name="com.shravan.alphab3t.gameActivity" />
        <activity android:name="com.shravan.alphab3t.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>